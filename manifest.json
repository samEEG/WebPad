{
  "manifest_version": 2,
  "name": "WebPad",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["/assets/js/content.js"]
    }

  ],
  "background": {
    "scripts": ["/assets/js/background.js"]
  },  
  "browser_action": {
    "default_icon": "/assets/imgs/highlight.png",
    "default_popup": "popup.html",   //This is code for the pop-script 
    "default_title": "Take note"
  }
}